{
  "name": "PlaceLocal BYOT",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Build Your Own Tearsheets - show placelocal ads on page and capture eTearsheets",
  "browser_action": {
    "default_icon": {                    // optional
      "19": "images/icon19.png"
    },
    "default_title": "PlaceLocal BYOT"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["mystyles.css"],
      "js": ["jquery-1.9.1.min.js", "jquery-ui.min.js", "myscript.js"],
      "run_at": "document_idle"
    }
  ]
}